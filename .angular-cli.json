      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../bower_components/bootstrap/dist/css/bootstrap.min.css",
        "../bower_components/font-awesome/css/font-awesome.min.css",
